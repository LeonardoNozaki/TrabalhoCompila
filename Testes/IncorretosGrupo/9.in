function fatorial(n : Int) -> Int{
	if n <= 0 {
		return 1;
	}
	else {
		return n*fatorial(n-1);
	}
}

function imprimir(before: Int, valor: Int, after: String) {
	var i : Int;
	i = 0;
	while i < before {
		write("*");
		i = i + 1;
	}
	writeln("");
	writeln(valor);
	writeln(after);
}

